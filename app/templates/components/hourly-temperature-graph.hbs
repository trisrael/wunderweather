{{!-  {{concat "0 0 " highestTemperature " 24"}}   
{{#with (get (find-extreme-hour temperatureHours) "temp.fahrenheight") as |highestTemperature|}}
  <svg version="1.2"
      xmlns="http://www.w3.org/2000/svg"
      xmlns:xlink="http://www.w3.org/1999/xlink"
      class="graph" aria-labelledby="title" role="img">
    {{#if title}}<title class="HourlyGraph-title">{{title}}</title>{{/if}}
    <g class="grid x-grid" class="HourlyGraph-xGrid">
      <line x1="0" x2="90" y1="5" y2={{highestTempHour}}></line>
    </g>
    <g class="grid y-grid" class="HourlyGraph-yGrid">
      <line x1="0" x2="24" y1={{highestTempHour}} y2={{highestTempHour}}></line>
    </g>
      <g class="labels x-labels">
        {{! Get unique hours in day-}}
        {{#with (union (map-by "civil" temperatureHours) (map-by "civil" temperatureHours)) as |hoursOfDay|}}
          {{#each hoursOfDay as |day index|}}
            <text x={{index}} y={{highestTemperature}}>{{day}}</text>
          {{/each}}
        {{/with}}
      <text x="400" y="440" class="label-title">Temperature (Â°F)</text>
    </g>
    <g class="labels y-labels">
      <text x="80" y="56">15</text>
      <text x="80" y="56">10</text>
      <text x="80" y="23">5</text>
      <text x="80" y="67">0</text>
      <text x="50" y="200" class="label-title">Hour of Day</text>
    </g>
    <g class="data" data-setname="Our first data set">
      <circle cx="4" cy="78" data-value="7.2" r="4"></circle>
      <circle cx="6" cy="23" data-value="8.1" r="4"></circle>
    </g>
  </svg>
{{/with}}
